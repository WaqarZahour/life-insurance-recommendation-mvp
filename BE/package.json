{
  "name": "life-insurance-recommendation-backend",
  "version": "1.0.0",
  "description": "Backend API for Life Insurance Recommendation MVP",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "docker:build": "docker build -t life-insurance-backend .",
